{
  "name": "@stellar/design-system",
  "version": "0.1.0-alpha.3",
  "author": "Stellar Development Foundation <hello@stellar.org>",
  "description": "Components for Stellar Development Foundationâ€™s design system",
  "license": "Apache-2.0",
  "repository": {
    "url": "git@github.com:stellar/stellar-design-system.git",
    "type": "git"
  },
  "main": "dist/final.min.js",
  "types": "dist/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "concurrently 'pretty-quick --staged' 'lint-staged' 'tsc --noEmit'",
      "post-merge": "yarn install-if-package-changed"
    }
  },
  "lint-staged": {
    "src/**/*.ts?(x)": [
      "eslint --fix --max-warnings 0"
    ]
  },
  "scripts": {
    "build": "webpack --mode production",
    "build:tsc": "tsc -p tsconfig.json",
    "prepare": "yarn build; yarn build:tsc",
    "install-if-package-changed": "git diff-tree -r --name-only --no-commit-id ORIG_HEAD HEAD | grep --quiet yarn.lock && yarn install || exit 0"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@stellar/eslint-config": "^1.0.5",
    "@stellar/prettier-config": "^1.0.1",
    "@stellar/tsconfig": "^1.0.2",
    "@svgr/webpack": "^5.5.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "concurrently": "^5.3.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.2",
    "path": "^0.12.7",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "configurable-date-input-polyfill": "^2.7.2",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
